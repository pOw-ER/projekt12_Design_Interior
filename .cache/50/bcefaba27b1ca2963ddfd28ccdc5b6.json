========================= */\n\nnav {\n  position: sticky;\n  width: 100%;\n  top: 0;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 20px 0;\n  background-color: #2e2e2e;\n}\nnav ul {\n  list-style: none;\n  display: flex;\n  align-items: center;\n}\nnav ul li {\n  padding-right: 100px;\n}\nnav ul li a {\n  text-decoration: none;\n  color: #fff;\n  font-size: 1.2rem;\n  font-family: 'Work Sans', sans-serif;\n}\nnav>a {\n  display: none;\n}\n\n/* =================================================================\n                      SECTION OUR STORY\n================================================================= */\n\nmain section:nth-of-type(1) {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  padding-top: 100px;\n  padding-bottom: 100px;\n  background-color: #2e2e2e;\n  color: #fff;\n\n}\n\nmain section:nth-of-type(1) div {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-right: 40px;\n  margin-left: 40px;\n}\nmain section:nth-of-type(1) div h2 {\n  font-size: 2rem;\n  font-weight: normal;\n}\nmain section:nth-of-type(1) div p {\n  text-align: justify;\n  letter-spacing: 2px;\n  word-spacing: 4px;\n  font-size: 1rem;\n  font-family: 'Work Sans', sans-serif;\n  font-weight: 300;\n  line-height: 30px;\n}\n/* =================================================================\n                      SECTION FEATURED\n================================================================= */\nmain section:nth-of-type(2) {\n  padding-top: 150px;\n  padding-bottom: 150px;\n}\n\nmain section:nth-of-type(2) div:nth-of-type(1){\n  text-align: center;\n}\nmain section:nth-of-type(2) div:nth-of-type(1) h2 {\n  font-size: 2rem;\n  font-weight: normal;\n}\nmain section:nth-of-type(2) div:nth-of-type(1) p {\n  text-align: justify;\n  width: 50%;\n  margin: 0 auto;\n  letter-spacing: 2px;\n  word-spacing: 4px;\n  font-size: 1rem;\n  font-family: 'Work Sans', sans-serif;\n  font-weight: 300;\n  line-height: 30px;\n}\n/* =================================================================\n                      SECTION FEATURED PHOTOS\n================================================================= */\n\nmain section:nth-of-type(2) div:nth-of-type(2) {\n  display: flex;\n  justify-content: space-evenly;\n  flex-wrap: wrap;\n}\nmain section:nth-of-type(2) div:nth-of-type(2) figure {\n  margin: 50px 0;\n  width: 25%;\n  min-width: 300px;\n}\nmain section:nth-of-type(2) div:nth-of-type(2) figure img {\n  width: 100%;\n}\nmain section:nth-of-type(2) div:nth-of-type(2) figure figcaption {\n  text-align: center;\n  font-weight: bold;\n  color: #999;\n  letter-spacing: 1.2px;\n}\n/* =================================================================\n                      SECTION FEATURED VIDEO\n================================================================= */\n\nmain section:nth-of-type(2) div:nth-of-type(3) {\n  text-align: center;\n}\nmain section:nth-of-type(2) div:nth-of-type(3) iframe{\n  width: 30%;\n  height: 30vh;\n}\n/* =================================================================\n                      FOOTER\n================================================================= */\nfooter {\n  background-color: #2e2e2e;\n  color: #fff;\n  padding-top: 120px;\n}\nfooter section {\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n}\nfooter section article{\n  margin: 0 auto;\n  width: 40%;\n  padding: 50px;\n  text-align: center;\n}\nfooter section article h2 {\n  font-size: 2rem;\n  font-weight: normal;\n}\nfooter section article p {\n  text-align: justify;\n  letter-spacing: 2px;\n  word-spacing: 4px;\n  font-size: 1rem;\n  font-family: 'Work Sans', sans-serif;\n  font-weight: 300;\n  line-height: 30px;\n}\n/* =================================================================\n                      FOOTER CONTACT FORM\n================================================================= */\nfooter section div{\n  width: 40%;\n